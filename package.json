{
  "version": "0.1.0",
  "license": "MIT",
  "description": "Javascript/Typescript Library to mix images.",
  "keywords": [
    "images",
    "art",
    "node"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && npm run copy-assets-file",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "copy-assets-file": "copyfiles assets/* dist/"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always",
    "eqeqeq": "smart"
  },
  "homepage": "https://github.com/danilobassi8/image-mixer#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/danilobassi8/image-mixer"
  },
  "name": "image-mixer",
  "author": "Bassi, Danilo",
  "module": "dist/image-mixer.esm.js",
  "size-limit": [
    {
      "path": "dist/image-mixer.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/image-mixer.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "copyfiles": "^2.4.1",
    "husky": "^8.0.1",
    "mock-fs": "^5.1.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "canvas": "^2.9.1",
    "merge-images": "^2.0.0"
  }
}
